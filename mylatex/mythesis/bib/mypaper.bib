@article{28,
   author = {董延朋 ， 孔祥春},
   title = {影响地质雷达工作的因素分析},
   journal = {物探装备},
   number = {02},
   pages = {130-133+155},
   abstract = {广泛用于工程勘查、水文调查、考古探测领域的地质雷达技术日趋成熟。针对不同的勘察目标、不同的地下介质,科学地设计其探测深度和分辨率是技术应用的关键。为此,必须分析、掌握雷达工作的三个重要参数:环境电导率、介电常数和探测频率。环境电导率σ是表征介质导电能力的参数,它决定了电磁波在介质中的穿透深度,其穿透深度随电导率的增加而减小,并与土壤中的含水量有密切的关系;当σ<10-7S/m,并满足介电极限条件时,电磁波衰减小,最适宜发挥地质雷达应用效果。介电常数是影响应用效果的另一个重要因素,高频电磁波在介质中的传播速度主要取决于介质的相对介电常数,而反射信号的强弱取决于介电常数的差异。探测频率不但是制约探测深度的一个关键因素,同时也决定了探测的分辨率;探测频率越高,探测深度越浅,探测的垂直分辨率和水平分辨率越高。},
   keywords = {地质雷达
环境电导率
介电常数
探测频率},
   year = {2007}
}

@phdthesis{29,
   author = {阿不都拉・阿不力孜},
   title = {基于探地雷达的浅层地下水埋深探测研究},
   university = {新疆大学},
   type = {硕士},
   abstract = {地下水是干旱半干旱地区最重要的自然资源之一,地下水的水位分布及其水质变化导致植被退化、沙漠化、土壤盐渍化等环境问题,影响着区域农业生产和生态环境的可持续发展。因此,快速和准确地获取地下水埋深以及其分布信息显得尤为重要。 	本文的主要目的在于借助探地雷达设备探测浅层地下水埋深。本文以渭干河库车河三角洲绿洲东部平原区作为研究区,使用探地雷达设备ZOND-12E,选取100 MHz频率的箱式屏蔽天线和38 MHz, 75 MHz和150 MHz频率的条形非屏蔽天线进行数据采集。因研究区地表较为粗糙,不宜于直接使用箱式天线进行数据采集,因此,制作了探地雷达拖动装置使得设备稳定运作。为后期的验证需求人工挖坑获取了实测地下水埋深。 	通过处理和分析探地雷达A-scan数据中电磁波信号在地表中的传播和反射特性及反射频率的变化,定性半定量地获取了浅层地下水埋深,并用地下水埋深实测值进行了验证。结果表明:探地雷达进行探测时,分析雷达信号反射特性并结合探测点地质的认识,可以准确、快速和有效地获取地下水埋深信息。选择天线频率及其类型应根据实地探测点的地表电磁物理特性、探测方案、地表粗糙度和探测深度以及分辨率要求选取合适频率的天线。},
   keywords = {探地雷达
浅层地下水埋深
共位移法},
   year = {2011}
}

