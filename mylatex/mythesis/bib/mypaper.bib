@article{8,
   author = {胡文志 ， 马海涛 ， 谢振华 ， 李全明},
   title = {三维高密度电法在尾矿库隐患检测中的应用},
   journal = {中国安全生产科学技术},
   number = {09},
   pages = {64-67},
   abstract = {三维高密度电阻率法是在二维的基础上发展起来的,克服了二维高密度电法不能直观地展示目标异常区域的走向、空间位置与形态的不足。将三维高密度电法应用于尾矿库隐患检测中,可得到丰富的地电数据,利用三维电阻率处理软件对地电数据进行反演,实现了尾矿坝三维地质数据的可视化。通过三维电阻率等值面图和三维电阻率动态截面图,可以直观、可靠的了解到尾矿坝浸润线的分布规律和坝体异常地质情况,为尾矿库的风险评估工作提供科学依据。},
   keywords = {三维高密度电法
尾矿坝
浸润线
隐患检测},
   year = {2011}
}

